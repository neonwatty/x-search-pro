{
  "name": "x-search-tabs",
  "version": "1.0.0",
  "description": "Chrome extension to simplify X/Twitter advanced search",
  "scripts": {
    "test:install": "playwright install chromium",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:unit": "playwright test --project=unit",
    "test:workflows": "playwright test tests/e2e/workflows/",
    "test:setup": "playwright test --project=setup",
    "test:report": "playwright show-report"
  },
  "keywords": [
    "chrome-extension",
    "twitter",
    "x",
    "search",
    "browser-extension"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@types/chrome": "^0.0.254",
    "@types/node": "^20.10.0",
    "dotenv": "^16.3.1",
    "typescript": "^5.3.3"
  }
}